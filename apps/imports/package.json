  "version": "2.15.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "imports",
    "commercelayer"
  ],
  "scripts": {
    "dev": "vite",
