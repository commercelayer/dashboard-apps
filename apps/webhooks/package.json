    "phosphor-react": "^1.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.54.2",
    "swr": "^2.3.2",
    "type-fest": "^4.35.0",
    "wouter": "^3.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commercelayer/app-builder": "workspace:*",
    "@commercelayer/eslint-config-ts-react": "^2.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.13.4",
    "@types/react": "^19.0.10",
