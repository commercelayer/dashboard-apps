{
  "private": true,
  "name": "app-webhooks",
  "description": "A Commerce Layer Single Page Application to manage webhooks with a nice GUI.",
  "version": "2.15.0",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "webhooks",
    "commercelayer"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
