  "version": "3.9.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "imports",
    "commercelayer"
  ],
  "scripts": {
    "dev": "vite",
