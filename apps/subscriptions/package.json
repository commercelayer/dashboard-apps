{
  "private": true,
  "name": "app-subscriptions",
  "description": "A Commerce Layer Single Page Application to manage subscriptions with a nice GUI.",
  "version": "2.8.0",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "subscriptions",
    "commercelayer"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "ts:check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepare": "touch ./public/config.local.js"
  },
  "dependencies": {
    "@commercelayer/app-elements": "^3.2.0",
    "@commercelayer/sdk": "^6.25.0",
    "@hookform/resolvers": "^3.9.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "swr": "^2.2.5",
    "type-fest": "^4.26.1",
    "wouter": "^3.3.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commercelayer/app-builder": "workspace:*",
    "@commercelayer/eslint-config-ts-react": "^1.4.5",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cronstrue": "^2.50.0",
    "eslint": "^8.57.0",
    "jsdom": "^25.0.1",
    "rollup-plugin-external-globals": "^0.12.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20"
  }
}
