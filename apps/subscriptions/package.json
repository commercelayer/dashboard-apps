{
  "private": true,
  "name": "app-subscriptions",
  "description": "A Commerce Layer Single Page Application to manage subscriptions with a nice GUI.",
  "version": "3.19.2",
  "license": "MIT",
  "keywords": [
    "reactjs",
    "jamstack",
    "headless",
    "ecommerce",
    "api",
    "subscriptions",
    "commercelayer"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "ts:check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepare": "touch ./public/config.local.js"
  },
  "dependencies": {
    "@commercelayer/app-elements": "6.11.0",
    "@commercelayer/sdk": "6.52.0",
    "@hookform/resolvers": "^3.10.0",
    "lodash-es": "^4.17.23",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.62.0",
    "swr": "^2.4.0",
    "type-fest": "^5.4.4",
    "wouter": "^3.9.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@commercelayer/app-builder": "workspace:*",
    "@commercelayer/eslint-config-ts-react": "^2.6.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "22.13.4",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "cronstrue": "^3.12.0",
    "eslint": "^8.57.1",
    "i18next": "^25.8.13",
    "jsdom": "^27.4.0",
    "rollup-plugin-external-globals": "^0.13.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
